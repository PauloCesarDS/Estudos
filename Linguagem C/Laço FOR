#include<stdio.h>
#include<stdlib.h>

main()
{	//atividade:

int i, num, maior;
	
	printf("***Verifique o maior valor dentre (5) os digitados***\n");
	
	// laco para controle de loop
	for(i =1; i <=5; i++)
{
	printf("\nDigite o numero:");
	scanf("%d", &num);	
	system("cls");	
	
	//comparacao dentro do for
	if(num >= maior)
	{
	maior = num;
	}
}
	printf("O maior numero digitado foi: %d",maior);
}
